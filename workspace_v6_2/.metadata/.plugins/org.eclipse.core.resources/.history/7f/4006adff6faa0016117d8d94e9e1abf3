#include "hal_wdt.h"
#include "hal_pmm.h"
#include "hal_general.h"
#include "interrupts.h"
#include "hal_usc.h"
#include "hal_timerB0.h"
#include "hal_timerA1.h"
#include "hal_gpio.h"
#include "hal_usciB1.h"

USCIB1_SPICom SpiCom;




void HAL_Init(void)
{
	HAL_Wdt_Init();
	HAL_PMM_Init ();
	HAL_GPIO_Init();
	HAL_UCS_Init();
	HAL_TimerB0_Init();
    HAL_TimerA1_Init();
    HAL_USCIB1_Init();
    SpiCom.TxData.len=3;
    SpiCom.TxData.Data[0]=200;
    SpiCom.TxData.Data[1]=100;
    SpiCom.TxData.Data[2]=50;
    HAL_USCIB1_Transmit();

}

